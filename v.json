boot_link=""
if [ "$boot_img" = "true" ]; then
  boot_link="https://github.com/RecSpeed/firmwareextrs/releases/download/auto/boot_img_${rom_name}.zip"
fi

FILE="v.json"
NEW_ENTRY='"'$rom_name'": {
  "boot_img_zip": "'$boot_img'",
  "init_boot_img_zip": "'$init_boot'",
  "vendor_boot_img_zip": "'$vendor_boot'",
  "app_settings_zip": "'$settings_apk'",
  "boot_img_link": "'$boot_link'"
}'
